{
  "UI Libraries": {
    "Install UI Library": {
      "shadcn/ui": {
        "Install With": {
          "npm": "npm install shadcn@latest",
          "pnpm": "pnpm add shadcn@latest",
          "yarn": "yarn add shadcn@latest",
          "bun": "bun add shadcn@latest"
        }
      },
      "Tailwind CSS": {
        "Install With": {
          "npm": "npm install -D tailwindcss postcss autoprefixer",
          "pnpm": "pnpm add -D tailwindcss postcss autoprefixer",
          "yarn": "yarn add -D tailwindcss postcss autoprefixer",
          "bun": "bun add -d tailwindcss postcss autoprefixer"
        },
        "Init": {
          "npm": "npx tailwindcss init -p",
          "pnpm": "pnpm exec tailwindcss init -p",
          "yarn": "yarn tailwindcss init -p",
          "bun": "bun x tailwindcss init -p"
        }
      },
      "Material UI": {
        "Install With": {
          "npm": "npm install @mui/material @emotion/react @emotion/styled",
          "pnpm": "pnpm add @mui/material @emotion/react @emotion/styled",
          "yarn": "yarn add @mui/material @emotion/react @emotion/styled",
          "bun": "bun add @mui/material @emotion/react @emotion/styled"
        }
      },
      "Bootstrap": {
        "Install With": {
          "npm": "npm install bootstrap",
          "pnpm": "pnpm add bootstrap",
          "yarn": "yarn add bootstrap",
          "bun": "bun add bootstrap"
        }
      }
    },
    "Add UI Components (shadcn/ui)": {
      "Button": "npx shadcn add button",
      "Input": "npx shadcn add input",
      "Dialog": "npx shadcn add dialog",
      "Alert Dialog": "npx shadcn add alert-dialog",
      "Tooltip": "npx shadcn add tooltip",
      "Card": "npx shadcn add card",
      "Sheet": "npx shadcn add sheet",
      "Dropdown Menu": "npx shadcn add dropdown-menu",
      "Toast": "npx shadcn add toast"
    }
  },

  "Development": {
    "Start Server": {
      "npm": "npm run dev",
      "pnpm": "pnpm dev",
      "yarn": "yarn dev",
      "bun": "bun run dev"
    },
    "Build Project": {
      "npm": "npm run build",
      "pnpm": "pnpm build",
      "yarn": "yarn build",
      "bun": "bun run build"
    },
    "Preview Build": {
      "npm": "npm run preview",
      "pnpm": "pnpm preview",
      "yarn": "yarn preview",
      "bun": "bun run preview"
    }
  },

  "Databases": {
    "Install Database Client": {
      "Convex": "npm install convex",
      "NeonDB": "npm install neon",
      "MongoDB": "npm install mongodb",
      "PostgreSQL": "npm install pg",
      "MySQL": "npm install mysql2",
      "Redis": "npm install ioredis",
      "Supabase": "npm install @supabase/supabase-js",
      "Firebase": "npm install firebase"
    }
  },

  "Prisma": {
    "Install Prisma": {
      "Prisma CLI": "npm install prisma --save-dev",
      "Prisma Client": "npm install @prisma/client"
    },
    "Setup & Commands": {
      "Prisma Init": "npx prisma init",
      "Prisma Migrate Dev": "npx prisma migrate dev",
      "Prisma Generate": "npx prisma generate",
      "Prisma Studio": "npx prisma studio"
    }
  },

  "Docker": {
    "Docker Commands": {
      "Docker Build": "docker build -t your-image-name .",
      "Docker Run": "docker run -p 3000:3000 your-image-name",
      "Docker Compose Up": "docker compose up",
      "Docker Compose Down": "docker compose down",
      "Docker List Containers": "docker ps -a",
      "Docker Stop All": "docker stop $(docker ps -aq)",
      "Docker Remove All Containers": "docker rm $(docker ps -aq)"
    }
  },

  "Git": {
    "Git Basic Commands": {
      "Git Init": "git init",
      "Git Clone": "git clone <repository-url>",
      "Git Add All": "git add .",
      "Git Commit": "git commit -m 'your message'",
      "Git Push": "git push origin main",
      "Git Pull": "git pull origin main",
      "Git Checkout New Branch": "git checkout -b new-branch",
      "Git Status": "git status",
      "Git Log": "git log --oneline"
    }
  },

  "Supabase": {
    "Supabase Commands": {
      "Login": "npx supabase login",
      "Init": "npx supabase init",
      "Start Local": "npx supabase start",
      "DB Push": "npx supabase db push"
    }
  },

  "Firebase": {
    "Firebase Commands": {
      "Login": "firebase login",
      "Init": "firebase init",
      "Deploy": "firebase deploy",
      "Start Emulator": "firebase emulators:start"
    }
  },

  "CLI Tools": {
    "Create New Project": {
      "Vite": "npm create vite@latest",
      "Next.js": "npx create-next-app@latest",
      "Astro": "npm create astro@latest",
      "SvelteKit": "npm create svelte@latest",
      "Remix": "npx create-remix@latest"
    }
  },

  "Convex DB": {
    "Convex Commands": {
      "Dev Server": "npx convex dev",
      "Codegen": "npx convex codegen",
      "Deploy": "npx convex deploy",
      "Import": "npx convex import",
      "Export": "npx convex export"
    }
  },

  "Testing & Linting": {
    "Testing Commands": {
      "Run Tests": "npm test",
      "Lint Code": "npm run lint"
    }
  },

  "Deployment": {
    "Deploy With": {
      "Vercel": "vercel --prod",
      "Netlify": "netlify deploy --prod",
      "Railway": "railway up"
    }
  }
}
